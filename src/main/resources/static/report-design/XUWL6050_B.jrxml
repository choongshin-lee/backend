	<parameter name="companyCd" class="java.lang.String"/>
	<parameter name="divisionCd" class="java.lang.String"/>
	<parameter name="departureNo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
	ROWNUM AS SEQ,MSPR.*
	FROM
	(
	SELECT 
		MPD.PACKING_ID AS DEPARTURE_NO,
		MSOM.CUSTOMER_CD,
		MSOM.DESTINATION_CD,
		MSOM.FORWARDER_CD,
		MSOM.NOTIFY_PARTY_CD,
		MSOM.INVOICE_NO,
		MSOM.INVOICE_DATE,
		MSOM.LC_NO,
		MSOM.LC_DATE,
		MSOM.LC_ISSUING_BANK,
		MSOM.LOADING_PLACE_CD,
		MSOM.DESTINATION_PORT_CD,
		MSOM.DELIVERY_CONDITIONS,
		MSOM.PAYMENT_TERMS,
		MSOM.SHIPPING_REMARK,
		MSOM.COMMODITY,
		MSOM.GOODS_DESCRIPTION,
		MSOM.SHIPPING_DUE_DATE,
		MSOM.SHIPPING_MARK,
		MSOM.CURRENCY_CD,
		MSOM.EXCHANGE_RATE,
		MPR.ITEM_ID,
		MPR.LOT_ID,
		SUM(MPR.ITEM_QTY) AS ITEM_QTY,
		MPR.ITEM_UOM,
		MPR.PKG_ITEM_ID2,
		MPR.PKG_NO2,
		MPR.PKG_TOTAL_CNT2,
		MPR.PACKING_SPEC2,
		SUM(MPR.NET_WEI2) AS NET_WEI2,
		MPR.NET_WEI_UOM2,
		SUM(MPR.GROSS_WEI2) AS GROSS_WEI2,
		MPR.GROSS_WEI_UOM2,
		MPR.PACKING_VOLUME2,
		MPR.PACKING_VOLUME_UOM2,
		MI.ITEM_NM AS PKG_ITEM_NM2,
		CSTM.PARTNER_NM AS CSTM__NM,
		CSTM.ADDRESS AS CSTM__ADDR,
		CSTM.TEL_NO AS CSTM_PHONE,
		NTFY.PARTNER_NM AS NTFY__NM,
		NTFY.ADDRESS AS NTFY__ADDR,
		NTFY.TEL_NO AS NTFY_PHONE,
		POL.PARTNER_NM AS POL__NM,		
		POD.PARTNER_NM AS POD__NM,
		SUM(MPR.NET_WEI2)/SUM(MPR.ITEM_QTY) AS NET_WEI_UNIT
	FROM MOM_MASTER.MOM_PACKING_DTL MPD , MOM_MASTER.MOM_PACKING_RESULT MPR,MOM_ITEM MI,MOM_PARTNER CSTM,MOM_PARTNER NTFY,MOM_PARTNER POL,MOM_PARTNER POD,
			( SELECT * FROM
					(
					SELECT ROW_NUMBER() OVER(PARTITION BY MPD.PACKING_ID  ORDER BY MPD.PACKING_ID ASC ) AS RNUM,
						MSOM.*,MPD.PACKING_ID  FROM MOM_MASTER.MOM_SHIPPING_ORDER_MST MSOM,MOM_MASTER.MOM_PACKING_DTL MPD
							WHERE 1=1
							AND MSOM.COMPANY_CD = MPD.COMPANY_CD 
							AND MSOM.DIVISION_CD = MPD.DIVISION_CD 
							AND MSOM.SHIPPING_NO = MPD.REFERENCE_NO
							AND MPD.COMPANY_CD = $P{companyCd}
							AND MPD.DIVISION_CD = $P{divisionCd}
							AND MPD.PACKING_ID = $P{departureNo}							
--							AND MPD.PACKING_ID = 'PAC231011000001'
					)
					WHERE 1=1
						AND RNUM = 1
			) MSOM
	WHERE 1=1
	AND MPD.COMPANY_CD = MPR.COMPANY_CD 
	AND MPD.DIVISION_CD = MPR.DIVISION_CD 
	AND MPD.PACKING_ID = MPR.PACKING_ID 
	AND MPD.SEQ = MPR.SEQ
	AND MPD.COMPANY_CD = MSOM.COMPANY_CD
	AND MPD.DIVISION_CD = MSOM.DIVISION_CD
	AND MPD.PACKING_ID = MSOM.PACKING_ID
	AND MPR.PKG_ITEM_ID2 = MI.ITEM_ID (+)
	AND MSOM.COMPANY_CD = CSTM.COMPANY_CD (+)
	AND MSOM.DIVISION_CD = CSTM.DIVISION_CD (+)
	AND MSOM.CUSTOMER_CD = CSTM.PARTNER_CD(+)
	AND MSOM.COMPANY_CD = NTFY.COMPANY_CD (+)
	AND MSOM.DIVISION_CD = NTFY.DIVISION_CD (+)
	AND MSOM.NOTIFY_PARTY_CD  = NTFY.PARTNER_CD(+)
	AND MSOM.COMPANY_CD = POL.COMPANY_CD (+)
	AND MSOM.DIVISION_CD = POL.DIVISION_CD (+)
	AND MSOM.LOADING_PLACE_CD  = POL.PARTNER_CD(+)
	AND MSOM.COMPANY_CD = POD.COMPANY_CD (+)
	AND MSOM.DIVISION_CD = POD.DIVISION_CD (+)
	AND MSOM.DESTINATION_PORT_CD  = POD.PARTNER_CD(+)		
	GROUP BY
		MPD.PACKING_ID,
		MSOM.CUSTOMER_CD,
		MSOM.DESTINATION_CD,
		MSOM.FORWARDER_CD,
		MSOM.NOTIFY_PARTY_CD,
		MSOM.INVOICE_NO,
		MSOM.INVOICE_DATE,
		MSOM.LC_NO,
		MSOM.LC_DATE,
		MSOM.LC_ISSUING_BANK,
		MSOM.LOADING_PLACE_CD,
		MSOM.DESTINATION_PORT_CD,
		MSOM.DELIVERY_CONDITIONS,
		MSOM.PAYMENT_TERMS,
		MSOM.SHIPPING_REMARK,
		MSOM.COMMODITY,
		MSOM.GOODS_DESCRIPTION,
		MSOM.SHIPPING_DUE_DATE,
		MSOM.SHIPPING_MARK,
		MSOM.CURRENCY_CD,
		MSOM.EXCHANGE_RATE,
		MPR.ITEM_ID,
		MPR.LOT_ID,
		MPR.ITEM_UOM,
		MPR.PKG_ITEM_ID2,
		MPR.PKG_NO2,
		MPR.PKG_TOTAL_CNT2,
		MPR.PACKING_SPEC2,
		MPR.NET_WEI_UOM2,
		MPR.GROSS_WEI_UOM2,
		MPR.PACKING_VOLUME2,
		MPR.PACKING_VOLUME_UOM2,
		MI.ITEM_NM,
		CSTM.PARTNER_NM,
		CSTM.ADDRESS,
		CSTM.TEL_NO,
		NTFY.PARTNER_NM,
		NTFY.ADDRESS,
		NTFY.TEL_NO,
		POL.PARTNER_NM,		
		POD.PARTNER_NM
		ORDER BY MPR.PKG_NO2 ASC , MPR.LOT_ID ASC
	) MSPR]]>
	</queryString>
