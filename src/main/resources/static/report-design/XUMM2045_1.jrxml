	<parameter name="companyCd" class="java.lang.String"/>
	<parameter name="divisionCd" class="java.lang.String"/>
	<parameter name="departureNo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT			MPD.PO_SEQ AS PO_SEQ,
				MPM.STATE,
				MPM.PO_NO AS DEPARTURE_NO,
				MPM.DESCRIPTION ,
				TO_CHAR(MPM.CONFIRM_DATE,'YYYY-MM-DD') AS PO_DATE,
				TO_CHAR(MPD.PO_RECEIPT_DATE,'YYYY-MM-DD') AS PO_RECEIPT_DATE,
				TO_CHAR(MPD.DELIVER_CONFIRM_DATE,'YYYY-MM-DD') AS DELIVER_CONFIRM_DATE,
				TO_CHAR(MPM.PO_DUE_DATE,'YYYY-MM-DD') AS PO_DUE_DATE,
				MPD.ITEM_ID AS ITEM_ID,
				MI.ITEM_NM AS ITEM_NM,
				MI.ITEM_SPEC AS ITEM_SPEC,
				MI.MATERIAL_PROPERTIES AS MATERIAL_PROPERTIES,
				MI.DRAWING_NO AS DRAWING_NO,
				MI.UOM AS INV_UOM,
				MPD.PURCHASE_UOM AS PO_UOM,
				MPD.PO_QTY AS PO_QTY,
				MOM_STOCK_PKG.FN_GET_ITEM_CONVERSION_RATE( MPD.COMPANY_CD ,MPD.DIVISION_CD ,MPD.ITEM_ID,'P')* MPD.PO_QTY AS INV_QTY,
				MPD.UNIT_PRICE ,
				MPD.AMOUNT ,
				MOM_COMMON_PKG.FN_GET_TAX_RATE(MPD.COMPANY_CD,MPD.DIVISION_CD,MP.TAX_TYPE)* MPD.AMOUNT AS TAX_AMOUNT,
				MPM.CURRENCY_CD,
				MC.ADDRESS AS MD_ADDRESS,
				MC.BUSINESS_NO AS MC_BUSINESS_NO,
				MC.BUSINESS_TYPE AS MD_BUSINESS_TYPE,
				MC.BUSINESS_ITEM AS MD_BUSINESS_ITEM,
				MC.TEL_NO AS MD_TEL_NO,
				MC.FAX_NO AS MD_FAX_NO,
				MC.COMPANY_NM AS MC_COMPANY_NM,
				MC.REPRESENTATIVE_NM AS MC_REPRESENTATIVE_NM,
				MP.ADDRESS AS MP_ADDRESS,
				MP.BUSINESS_NO AS MP_BUSINESS_NO,
				MP.BUSINESS_TYPE AS MP_BUSINESS_TYPE,
				MP.BUSINESS_ITEM AS MP_BUSINESS_ITEM,
				MP.TAX_TYPE AS TAX_TYPE,
				MP.TEL_NO AS MP_TEL_NO,
				MP.FAX_NO AS MP_FAX_NO,
				MP.PARTNER_NM AS MP_VENDOR_NM ,
				MP.REPRESENTATIVE_NM AS MP_REPRESENTATIVE_NM
		FROM MOM_PO_MST MPM
		INNER JOIN MOM_PO_DTL MPD ON MPM.COMPANY_CD = MPD.COMPANY_CD 
											AND MPM.DIVISION_CD = MPD.DIVISION_CD 
											AND MPM.PO_NO = MPD.PO_NO 
		INNER JOIN MOM_ITEM MI	ON MPD.COMPANY_CD = MI.COMPANY_CD 
											AND MPD.DIVISION_CD = MI.DIVISION_CD 
											AND MPD.ITEM_ID = MI.ITEM_ID 
		INNER JOIN MOM_PARTNER MP	ON MPM.COMPANY_CD = MP.COMPANY_CD 
												AND MPM.DIVISION_CD = MP.DIVISION_CD 
												AND MPM.VENDOR_CD = MP.PARTNER_CD 
		INNER JOIN MOM_DIVISION MD	ON MPM.COMPANY_CD = MD.COMPANY_CD 
												AND MPM.DIVISION_CD = MD.DIVISION_CD 
		INNER JOIN MOM_COMPANY MC	ON MPM.COMPANY_CD = MC.COMPANY_CD 
				WHERE 1=1
--				AND MPM.STATE = 'POF'
				AND MPM.COMPANY_CD = $P{companyCd}
				AND MPM.DIVISION_CD = $P{divisionCd}
				AND MPM.PO_NO = $P{departureNo}
				ORDER BY MPD.PO_NO ASC, MPD.PO_SEQ ASC]]>
	</queryString>
