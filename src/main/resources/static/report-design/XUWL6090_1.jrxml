	<queryString language="SQL">
		<![CDATA[SELECT 
	'ttp://dklok.com/kr/%ec%a0%9c%ed%92%88%ec%86%8c%ea%b0%9c/' AS MILLSHEET_URL,
	MPR.LOT_ID,
	MSD.CUSTOMER_ITEM_ID ,
	MI.ITEM_NM,MI.ITEM_SPEC,
	MI.MATERIAL_PROPERTIES, 
	(SELECT CODE_NM FROM MOM_CODE MC 
	WHERE 1=1
	AND MC.COMPANY_CD = MI.COMPANY_CD 
	AND MC.DIVISION_CD = MI.DIVISION_CD
	AND MC.GROUP_CD = 'MD0003'
	AND MC.CODE = MI.MATERIAL_PROPERTIES
	) AS PROPERTIES_NM,
	TO_CHAR(MPD.PACKING_QTY,'FM990.999') AS  PACKING_QTY , 
	TO_CHAR(MPR.ITEM_QTY,'FM990.999') AS ITEM_QTY,
	MSOM.CUSTOMER_CD,
	MSD.SALES_ORDER_ID,
	MSD.CUSTOMER_PO_NO,
	(SELECT CODE_NM FROM MOM_CODE MC 
	WHERE 1=1
	AND MC.COMPANY_CD = MI.COMPANY_CD 
	AND MC.DIVISION_CD = MI.DIVISION_CD
	AND MC.GROUP_CD = 'SC0002'
	AND MC.CODE = MSOM.SHIP_MODE
	) AS SHIP_MODE,
	MPR.PACKING_ID ,
	MPR.PKG_NO1 ,
	MPR.PKG_TOTAL_CNT1 ,
	TO_CHAR(SYSDATE,'YYYY-MM-DD PM HH:MI:SS','NLS_DATE_LANGUAGE=AMERICAN') AS PRINT_DATE,
	MPR.UPDATE_BY,
	MU.USER_NM,
	MSD.CUSTOMER_ITEM_ID||';'||TO_CHAR(MPD.PACKING_QTY,'FM990.99')
	||';'||CUSTOMER_CD||';'||MSD.SALES_ORDER_ID||';'||MSD.CUSTOMER_PO_NO||';'||'운송방법'||';'||MPR.PACKING_ID||';'||
	TO_CHAR(SYSDATE,'YYYY-MM-DD PM HH:MI:SS','NLS_DATE_LANGUAGE=AMERICAN')||';' AS RIGHT_QR
FROM 	MOM_PACKING_RESULT MPR , 
		MOM_PACKING_DTL MPD , 
		MOM_SHIPPING_ORDER_DTL MSOD ,
		MOM_SHIPPING_ORDER_MST MSOM , 
		MOM_SO_DTL MSD , 
		MOM_ITEM MI ,
		MOM_USER MU
	WHERE 1=1
	AND MPR.PACKING_DETAIL_ID = MPD.PACKING_DETAIL_ID 
	AND MPD.REFERENCE_NO = MSOM.SHIPPING_NO(+)
	AND MPD.REFERENCE_DTL_NO = MSOD.SHIPPING_ORDER_ID (+)
	AND MSOD.REFERENCE_DTL_NO = MSD.SALES_ORDER_ID(+)
	AND MPR.ITEM_ID = MI.ITEM_ID 
	AND MPR.COMPANY_CD = MU.COMPANY_CD 
	AND MPR.DIVISION_CD = MU.DIVISION_CD 
	AND MPR.UPDATE_BY = MU.USER_NO
	AND MPR.COMPANY_CD = $P{companyCd}
	AND MPR.DIVISION_CD = $P{divisionCd}
	AND MPR.PACKING_ID = $P{departureNo}]]>
	</queryString>
